{
    "manifest_version": 3,
    "name": "inFilter",
    "version": "1.1.0",
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "description": "Remove ads from linkedin feed and block posts with certain keywords.",
    "permissions": [
      "storage"
    ],
    "host_permissions": [
    "*://*.linkedin.com/feed/"
  ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.linkedin.com/feed/"],
        "js": ["content.js"]
      }
    ]
  }